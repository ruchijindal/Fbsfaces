package com.smp.fbs;

/**
 *
 * @author smp
 */
public class CollectionDetail {
    long amount;
    String name;
    String day;

    public long getAmount() {
        return amount;
    }

    public void setAmount(long amount) {
        this.amount = amount;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getDay() {
        return day;
    }

    public void setDay(String day) {
        this.day = day;
    }
    

}

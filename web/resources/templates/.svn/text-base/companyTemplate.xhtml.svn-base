<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:p="http://primefaces.prime.com.tr/ui">

    <f:view>

        <h:head>
            <title>RE-ERP : <ui:insert name="title">title</ui:insert></title>
            <link href="${facesContext.externalContext.requestContextPath}/resources/css/default.css" rel="stylesheet" type="text/css" />
            <script type="text/javascript" src="${facesContext.externalContext.requestContextPath}/resources/FusionCharts/FusionCharts.js"></script>
            <body onload="fun()">
                <script type="text/javascript">
                //<![CDATA[
                 function fun() {
                   var datatableWidth = jQuery('div.ui-datatable > table').width();

                   jQuery('div.ui-datatable > div.ui-datatable-header').width(datatableWidth - 22);
                   jQuery('div.ui-datatable > div.ui-paginator').width(datatableWidth - 6);
                   jQuery('div.ui-datatable > div.ui-datatable-footer').width(datatableWidth - 22);
                };
                //]]>
                </script>
            </body>

        </h:head>
        <h:body>



            <p:layout fullPage="true">



                <p:layoutUnit id="top" position="top" height="56" gutter="0">
                    <ui:insert name="top">

                        <ui:include src="/jsfpages/common/top.xhtml"></ui:include>

                    </ui:insert>
                </p:layoutUnit>



                <p:layoutUnit id="left" position="left" width="210" scrollable="true" gutter="2px" minWidth="230">
                    <ui:insert name="sidebar">
                        <ui:include src="/jsfpages/common/c_sidebar.xhtml"></ui:include>
                    </ui:insert>
                </p:layoutUnit>



                <p:layoutUnit id="center" position="center" scrollable="true" gutter="2px">
                    <ui:insert name="content">
                        content box
                    </ui:insert>
                </p:layoutUnit>


                <p:layoutUnit id="bottom" position="bottom" height="35" scrollable="null" gutter="0" minWidth="960">
                    <ui:insert name="bottom">

                        <ui:include src="/jsfpages/common/bottom.xhtml"></ui:include>

                    </ui:insert>
                </p:layoutUnit>
            </p:layout>

        </h:body>


    </f:view>

</html>
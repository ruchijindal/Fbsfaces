<ui:composition  xmlns="http://www.w3.org/1999/xhtml"
                 xmlns:ui="http://java.sun.com/jsf/facelets"
                 xmlns:f="http://java.sun.com/jsf/core"
                 xmlns:h="http://java.sun.com/jsf/html"
                 xmlns:p="http://primefaces.prime.com.tr/ui">





    <h:form styleClass="ui-widget-header">



        <p:toolbar style="">
            <p:toolbarGroup align="left">
                <p:graphicImage url="/resources/images/realestate.png"/>
            </p:toolbarGroup>
            <p:toolbarGroup align="right">
                <table>
                    <tr>
                        <td colspan="2">
                    <h:outputText value="#{loginBean.companyName}" styleClass="companyName"/>
                        </td>
                    </tr>
                    <tr>
                        <td></td>
                        <td align="right">
                    <h:commandLink style="color: #0070a3 ;text-decoration: none;font-size: 12px;" value="#{loginBean.website}" action="#{loginBean.redirectWebsite(loginBean.website)}" ></h:commandLink>
                        </td>
                    </tr>
                </table>
            </p:toolbarGroup>
            <p:divider />
        </p:toolbar>
    </h:form>



</ui:composition>
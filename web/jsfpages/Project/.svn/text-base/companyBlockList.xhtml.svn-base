<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE composition PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<ui:composition xmlns:ui="http://java.sun.com/jsf/facelets"
                xmlns:f="http://java.sun.com/jsf/core"
                xmlns:h="http://java.sun.com/jsf/html"
                xmlns:p="http://primefaces.prime.com.tr/ui"
                template="./../../resources/templates/companyTemplate.xhtml"
                xmlns:c="http://java.sun.com/jsp/jstl/core">
    
    <ui:define name="title">
        Block List
    </ui:define>

   

    <ui:define name="content">
        <h:panelGrid columns="1">
            <p:breadCrumb>
                <p:menuitem value="" url="/faces/jsfpages/Dashboard/projectDashboard.xhtml" />
                <p:menuitem value="#{graphicalSearchBean.fbsProject.projName}" url="/faces/jsfpages/Project/projectList.xhtml" />
                <p:menuitem value="" url="#" />
            </p:breadCrumb>
        </h:panelGrid>
        <h:form>
            <h:panelGrid columns="3" cellpadding="15">
                <c:forEach var="block" items="#{graphicalSearchBean.fbsBlockList}">
                    <p:commandLink action="#{graphicalSearchBean.populateCompanyFloors(block)}" styleClass="a" >
                        <h4 align="center"></h4>
                        <img src="#{facesContext.externalContext.requestContextPath}/resources/images/flight.jpg"  alt="" style="padding-left: 32px;"/>
                        <h4 align="center">Block Name: #{block.blockName}</h4>
                    </p:commandLink>
                </c:forEach>
            </h:panelGrid>
        </h:form>
    </ui:define>

   

</ui:composition>
